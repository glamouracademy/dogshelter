<p id="notice"><%= notice %></p>

<strong>Name:</strong> <%= @agency.name %> <br>
<strong>City:</strong> <%= @agency.city %> <br>
<strong>State:</strong> <%= @agency.state %> <br>

<strong>Dogs:</strong>
<table>
  <tr>
    <th>Name</th>
    <th>Breed</th>
    <th>Weight</th>
    <th>Age</th>
  </tr>
<% @agency.dogs.each do |dog| %>
  <tr>
    <td><%= dog.name %></td>
    <td><%= dog.breed %></td>
    <td><%= dog.weight %></td>
    <td><%= dog.age %></td>
  </tr>
<% end %>
</table>

<br>

<%= link_to 'Edit', edit_agency_path(@agency) %> |
<%= link_to 'Back', agencies_path %>
